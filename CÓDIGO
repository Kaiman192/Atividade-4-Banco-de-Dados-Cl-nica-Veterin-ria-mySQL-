create table TIPOS_DE_DADOS(
CLIENT_ID INT auto_increment,
NOME VARCHAR(100),
TELEFONE VARCHAR(15),
EMAIL VARCHAR(100),
ENDEREÇO VARCHAR(255),
ANIMAL_ID INT auto_increment,
CLIENT_ID INT,
NOME VARCHAR(100),
ESPÉCIE VARCHAR(50),
RAÇA VARCHAR(50),
DATA_NASCIMENTO DATE,
SEXO ENUM('M','F'),
VETERINARIO_ID INT AUTO_INCREMENT,
NOME VARCHAR(100),
ESPECIALIDADE VARCHAR(50),
TELEFONE VARCHAR(15),
EMAIL VARCHAR(100),
CONSULTA_ID INT auto_increment,
ANIMAL_ID INT,
VATERINARIO_ID INT,
DATA_CONSULTA DATETIME,
DIAGNÓSTICO TEXT,
TRATAMENTO_ID INT auto_increment,
CONSULTA_ID INT,
DESCRIÇÃO TEXT,
MEDICAMENTO VARCHAR(100),
DOSE VARCHAR(50),
PAGAMENTO_ID INT auto_increment,
CONSULTA_ID INT,
VALOR DECIMAL(10, 2),
DATA_PAGAMENTO DATE,
METODO_PAGAMENTO ENUM('DINHEIRO','CARTÃO','TRANSFERÊNCIA')
);

